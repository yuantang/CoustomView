        <attr name="roundColor" format="color" />
        <attr name="countTime" format="integer"></attr>
        <attr name="refreshTime" format="integer"></attr>
        <attr name="circleColor" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>
