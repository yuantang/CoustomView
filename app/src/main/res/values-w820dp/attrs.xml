<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RippleProgressView">
        <attr name="roundColor" format="color"></attr>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="progressColor" format="color"></attr>
        <attr name="progressMax" format="integer"></attr>
        <attr name="rippleColor" format="color"></attr>
        <attr name="rippleWidth" format="dimension"></attr>
        <attr name="rippleDuration" format="integer"></attr>
        <attr name="rippleSpeed" format="integer"></attr>
